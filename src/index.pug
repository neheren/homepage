		if urlInfo
			script.
				var content = "#{urlInfo.content}";
				var id = "#{urlInfo.id}";
				$(document).ready(()=>{
					if(content && id){
						switch(content){
							case 'projects': scrollNow('#projekter') 
								setTimeout(() => openPortfolio(id), 500);
						}
					}
				})
